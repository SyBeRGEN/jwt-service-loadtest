              <stringProp name="script">import java.time.LocalDate
import java.time.format.DateTimeFormatter

def html = prev.getResponseDataAsString()
def matcher = html =~ /&lt;option(?: selected=&quot;selected&quot;)? value=&quot;([^&quot;]+)&quot;/
def cities = matcher.collect { it[1] }

if (cities.size() &gt; 1) {
    def rand = new Random()

    // города
    def depart = cities[rand.nextInt(cities.size())]
    def arrive
    do {
        arrive = cities[rand.nextInt(cities.size())]
    } while (arrive == depart)

    // даты
    def today = LocalDate.now()
    def departDate = today.plusDays(rand.nextInt(7) + 1)
    def returnDate = departDate.plusDays(rand.nextInt(5) + 1)
    def fmt = DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;)

    // пассажиры
    def numPassengers = (rand.nextInt(5) + 1).toString()

    // предпочтения
    def seatPrefs = [&apos;Aisle&apos;, &apos;Window&apos;, &apos;None&apos;]
    def seatTypes = [&apos;First&apos;, &apos;Business&apos;, &apos;Coach&apos;]
    def seatPref = seatPrefs[rand.nextInt(seatPrefs.size())]
    def seatType = seatTypes[rand.nextInt(seatTypes.size())]

    // рандомная roundtrip: on или нет
    def includeRoundTrip = rand.nextBoolean()

    // сохранить переменные
    vars.put(&quot;depart&quot;, depart)
    vars.put(&quot;arrive&quot;, arrive)
    vars.put(&quot;departDate&quot;, departDate.format(fmt))
    vars.put(&quot;returnDate&quot;, returnDate.format(fmt))
    vars.put(&quot;numPassengers&quot;, numPassengers)
    vars.put(&quot;seatPref&quot;, seatPref)
    vars.put(&quot;seatType&quot;, seatType)

    // условно добавим roundtrip
    if (includeRoundTrip) {
        vars.put(&quot;roundtrip&quot;, &quot;on&quot;)
    } else {
        vars.remove(&quot;roundtrip&quot;) // вообще не отправлять
    }
}
