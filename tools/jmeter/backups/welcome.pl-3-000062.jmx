            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def rand = new Random()

// Список имён и фамилий
def firstNames = [&quot;Ivan&quot;, &quot;Anna&quot;, &quot;John&quot;, &quot;Maria&quot;, &quot;Alex&quot;, &quot;Kate&quot;, &quot;Dmitry&quot;, &quot;Olga&quot;]
def lastNames = [&quot;Ivanov&quot;, &quot;Smith&quot;, &quot;Brown&quot;, &quot;Petrova&quot;, &quot;Johnson&quot;, &quot;Volkov&quot;, &quot;Lee&quot;, &quot;Kuznetsova&quot;]

// Рандомизация имени и фамилии
def firstName = firstNames[rand.nextInt(firstNames.size())]
def lastName = lastNames[rand.nextInt(lastNames.size())]
vars.put(&quot;firstName&quot;, firstName)
vars.put(&quot;lastName&quot;, lastName)

// Адрес
def streets = [&quot;Lenina&quot;, &quot;Baker&quot;, &quot;Pushkina&quot;, &quot;Main&quot;, &quot;Gorky&quot;, &quot;Tverskaya&quot;]
def cities = [&quot;Moscow&quot;, &quot;London&quot;, &quot;Berlin&quot;, &quot;New York&quot;, &quot;Istanbul&quot;, &quot;Tokyo&quot;]
def address1 = &quot;${rand.nextInt(999) + 1} ${streets[rand.nextInt(streets.size())]} St&quot;
def address2 = &quot;${cities[rand.nextInt(cities.size())]}, ZIP ${10000 + rand.nextInt(90000)}&quot;
vars.put(&quot;address1&quot;, address1)
vars.put(&quot;address2&quot;, address2)

// Рандом кол-ва пассажиров от 1 до 5
def numPassengers = vars.get(&quot;numPassengers&quot;)

// Генерация pass1, pass2, ..., passN
for (int i = 1; i &lt;= numPassengers; i++) {
    def pFirst = firstNames[rand.nextInt(firstNames.size())]
    def pLast = lastNames[rand.nextInt(lastNames.size())]
    vars.put(&quot;pass${i}&quot;, &quot;${pFirst} ${pLast}&quot;)
}

for (int i = 1; i &lt;= numPassengers; i++) {
    sampler.addArgument(&quot;pass${i}&quot;, vars.get(&quot;pass${i}&quot;))
}

// Генерация случайной кредитки (16 цифр)
def creditCard = (1..16).collect { rand.nextInt(10) }.join()
vars.put(&quot;creditCard&quot;, creditCard)
sampler.addArgument(&quot;creditCard&quot;, creditCard)

// Генерация срока действия карты в формате MM/YY
def expMonth = String.format(&quot;%02d&quot;, rand.nextInt(12) + 1)
def expYear = String.valueOf(rand.nextInt(5) + 26) // 26 -&gt; 2026+
def expDate = &quot;${expMonth}/${expYear}&quot;
vars.put(&quot;expDate&quot;, expDate)
sampler.addArgument(&quot;expDate&quot;, expDate)

//roundtrip
def roundtrip = vars.get(&quot;roundtrip&quot;)
def returnFlight = vars.get(&quot;returnFlight&quot;)

if (roundtrip != null &amp;&amp; returnFlight != null &amp;&amp; returnFlight != &quot;NOT_FOUND&quot;) {
    sampler.addArgument(&quot;returnFlight&quot;, returnFlight)
}
</stringProp>
